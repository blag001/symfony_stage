<form role="form" method="post" {{ form_enctype(form) }}>
	<table class="table table-striped">
		<tr>
			<th>Mission</th>
			<td>{{ form_widget(form.mission) }}</td>
		</tr>
		<tr>
			<th>Note (sur 5)</th>
