{% extends "layout.html.twig" %}

{% block content %}
    <head>
	<title>Voici les informations de l'étudiant</title>
	</head>
	{% for etudiant in etudiant %}
	<p>Voici les informations de l'étudiant: <strong>{{ etudiant.nom }}</strong></p>
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Adresse</th>
				<th>Numero de telephone</th>
				<th>Mail</th>
				<th>Section</th>
			</tr>
		</thead>
		<tbody>
				<tr>
					<td>{{ etudiant.adresse }}</td>
					<td>{{ etudiant.tel}}</td>
					<td>{{ etudiant.mail}}</td>
					<td>{{ etudiant.section.libelle}}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
<p><a href="{{ path('Etudiant_ConsulterE' ) }}"> Retour</p>
{% endblock %}